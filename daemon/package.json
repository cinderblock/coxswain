    "chalk": "^2.4.1",
    "ngrok": "^3.1.0",
    "node": "^10.14.2",
    "server-starter": "^1.3.1",
    "socket.io": "^2.1.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
